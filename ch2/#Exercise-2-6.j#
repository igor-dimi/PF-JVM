.class Exercise-2-6

	.method public computeSum(FF)F
		.limit locals 3
		.limit stack 2
		fload_1
		fload_2
		fadd
		freturn
	.end method
	
	.method public computeSum(FFF)F
		.limit locals 4
		.limit stack 3
		fload_1
		fload_2
		fadd
		fload_3
		fadd
		freturn
	.end method
	
	.method public static statComputeSum(FF)F
		.limit locals 2
		fload_0
		fload_1
		fadd
		freturn
	.end method
	
.end class

